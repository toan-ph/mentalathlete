	<array>
		<string>UIInterfaceOrientationPortrait</string>
		<string>UIInterfaceOrientationPortraitUpsideDown</string>
		<string>UIInterfaceOrientationLandscapeLeft</string>
		<string>UIInterfaceOrientationLandscapeRight</string>
	</array>
	<key>CADisableMinimumFrameDurationOnPhone</key>
	<true/>
	<key>UIApplicationSupportsIndirectInputEvents</key>
	<true/>
	
	<!-- Hintergrund-Modi -->
	<key>UIBackgroundModes</key>
	<array>
		<string>fetch</string>
		<string>remote-notification</string>
	</array>
	
	<!-- URL-Schema für Deep Links -->
	<key>CFBundleURLTypes</key>
	<array>
		<dict>
			<key>CFBundleTypeRole</key>
			<string>Editor</string>
			<key>CFBundleURLName</key>
			<string>com.example.mentalathlete</string>
			<key>CFBundleURLSchemes</key>
			<array>
				<string>mentalathlete</string>
			</array>
		</dict>
	</array>
	
	<!-- Erlaubte URL-Schemes -->
	<key>LSApplicationQueriesSchemes</key>
	<array>
		<string>https</string>
		<string>http</string>
		<string>mentalathlete</string>
	</array>
	
	<!-- Universal Links Konfiguration -->
	<key>com.apple.developer.associated-domains</key>
	<array>
		<string>applinks:ussoltqbugyjhowubowz.supabase.co</string>
	</array>
	
	<!-- Flutter Deep Linking - Konsolidierte Konfiguration -->
	<key>FlutterDeepLinkingEnabled</key>
	<true/>
	<key>FlutterDeepLinkingScheme</key>
	<string>mentalathlete</string>
	<key>FlutterDeepLinkingPaths</key>
	<array>
		<string>/deeplink/reset-password</string>
		<string>/deeplink/auth-callback</string>
		<string>/auth/v1/callback</string>
	</array>
	
	<!-- Zusätzliche Konfiguration für bessere App-Performance -->
